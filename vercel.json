{  "version": 2,  "name": "bloggers-platform-server",  "builds": [    {      "src": "src/index.ts",      "use": "@vercel/node"    }  ],  "routes": [    {      "src": "/api/swagger-ui.css",      "dest": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.18.3/swagger-ui.min.css"    },    {      "src": "/api/swagger-ui-bundle.js",      "dest": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.18.3/swagger-ui-bundle.min.js"    },    {      "src": "/api/swagger-ui-standalone-preset.js",      "dest": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.18.3/swagger-ui-standalone-preset.min.js"    },    {      "src": "/(.*)",      "dest": "src/index.ts"    }  ]}